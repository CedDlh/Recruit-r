

<h2>Add a position</h2>

<h3>Please enter the position below </h3>

<div>
<%= simple_form_for @position, url: positions_path, method: :post do |f| %>
  <%= f.error_notification %>

  <%= f.input :name %>

  <%= f.input :skill %>

  <%= f.button :submit, class: "btn-primary" %>
  <%#= f.button :button, "Cancel", type: "submit", class: "btn-outline-secondary" %>
<% end %>
</div>




